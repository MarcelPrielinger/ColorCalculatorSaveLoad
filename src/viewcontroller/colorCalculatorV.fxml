<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
          prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="viewcontroller.ColorCalculatorC">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Red"/>
        <Label text="Green" GridPane.rowIndex="2"/>
        <Label text="Blue" GridPane.rowIndex="4"/>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#btn_RedPlus" text="+10" GridPane.columnIndex="1"/>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#btn_RedMinus" text="-10" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#btn_GreenPlus" text="+10" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label fx:id="lblColor" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Hex: 000000"
               GridPane.columnIndex="2" GridPane.rowIndex="3" GridPane.rowSpan="3"/>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#btn_GreenMinus" text="-10" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#btn_BluePlus" text="+10" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#btn_BlueMinus" text="-10" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <TextField fx:id="txtGreen" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   text="0" GridPane.rowIndex="3" disable="true"/>
        <TextField fx:id="txtRed" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   text="0" GridPane.rowIndex="1" disable="true"/>
        <TextField fx:id="txtBlue" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   text="0" GridPane.rowIndex="5" disable="true"/>
        <Button fx:id="btnColor" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" GridPane.columnIndex="2" GridPane.rowSpan="3"/>
    </children>
</GridPane>
